Xcursor.theme:    Vanilla-DMZ-AA

!urxvt color scheme:
URxvt*background: #151515
URxvt*foreground: #DEDEDE

URxvt*colorUL: #86a2b0

! black
URxvt*color0  : #2E3436
URxvt*color8  : #555753
! red
URxvt*color1  : #CC0000
URxvt*color9  : #EF2929
! green
URxvt*color2  : #4E9A06
URxvt*color10 : #8AE234
! yellow
URxvt*color3  : #C4A000
URxvt*color11 : #FCE94F
! blue
URxvt*color4  : #3465A4
URxvt*color12 : #729FCF
! magenta
URxvt*color5  : #75507B
URxvt*color13 : #AD7FA8
! cyan
URxvt*color6  : #06989A
URxvt*color14 : #34E2E2
! white
URxvt*color7  : #D3D7CF
URxvt*color15 : #EEEEEC

!urxvt fonts:
!URxvt.font: xtf:Monospace:pixelsize=10
!URxvt.boldFont: monaco:bold:size=10

!URxvt.font:     xft:Inconsolata:size=12:antialias=false
!URxvt*font:     xft:-*-Terminus-medium-*-*-*-*-*-*-*-*-*-*-*
URxvt*font: xft:DejaVu Sans Mono:pixelsize=12:antialias=true
!URxvt.font: xft:-*-Terminus-medium-*-*-*-*-140-*-*-*-*-*
!URxvt.boldFont: xft:-*-Terminus-bold-*-*-*-*-140-*-*-*-*-*


!urxvt extensions:
URxvt.perl-ext-common:  default,matcher,selection-to-clipboard

!urxvt scrolling options and cursor style:
URxvt*saveLines: 12000
URxvt*scrollstyle:plain
URxvt*scrollBar: false
URxvt*cursorBlink: false
URxvt*cursorUnderline: false

! do not scroll with output
URxvt*scrollTtyOutput: false

! scroll in relation to buffer (with mouse scroll or Shift+Page Up)
URxvt*scrollWithBuffer: true

! scroll back to the bottom on keypress
URxvt*scrollTtyKeypress: true
